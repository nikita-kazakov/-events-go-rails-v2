<!DOCTYPE html>
<html>
  <head>
    <title>EventsRails6 - <%=content_for(:title) %></title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <div class="row">
        <div class="col-2">
          <%=content_for(:side) %>
        </div>
        <div class="col-10">

          <%= render 'layouts/navbar' %>
          <%= render 'layouts/flashnotice' %>
          <%= yield %>
          <%= render 'layouts/footer' %>

        </div>

      </div>


    </div>


  </body>
</html>
